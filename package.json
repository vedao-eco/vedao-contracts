  "scripts": {
    "compile": "hardhat compile",
    "test:all": "npm run test -- --parallel",
    "test:hardhat": "cross-env NODE_ENV=test hardhat test",
    "test:forge": "forge test",
    "dev": "hardhat node --no-deploy",
    "format": "prettier --loglevel=silent --write .",
    "prepare": "husky install",
    "clean": "hardhat clean",
    "predeploy": "hardhat clean && hardhat compile",
    "size-contracts": "SKIP_LOAD=true npx hardhat size-contracts",
    "deploy:hardhat": "npm run predeploy && hardhat deploy --network hardhat",
    "deploy:dev": "hardhat deploy --network 44061",
    "deploy:rinkeby": "npm run predeploy && hardhat deploy --network rinkeby",
    "deploy:bsctest": "npm run predeploy && hardhat deploy --network 97",
    "deploy:testnet": "npm run predeploy && hardhat deploy --network testnet",
    "deploy:bsc": "hardhat deploy --network 56",
    "cp:artifact": "hardhat copy-artifacts --target \"../webapp/app/contract/contracts,../vedao-boss-web/src/contract/artifacts\""
  },
  "dependencies": {
    "dayjs": "^1.11.5",
    "keythereum": "^2.0.0",
